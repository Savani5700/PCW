    "getParent": {
      "returnType": "string[]",
      "description": "Return Parent Nodename for Node",
      "parameters": [
        {
          "name": "IDorDEC",
          "type": "string",
          "description": "get parent ID or DEC"            
        }
      ],
      "body": "if(IDorDEC === 'ID'){return this.ptid;}else if(IDorDEC === 'DEC'){return this.ptdec;}"
    },
    "getColumnValue": {
      "returnType": "string[]",
      "description": "Get Return Data",
      "parameters": [
        {
          "name": "FieldName",
          "type": "string",
          "description": "Set FieldName"
        }
      ],
      "body": "if(this.Field1_Name === FieldName){return this.Field1_Value;}else if(this.Field2_Name === FieldName){return this.Field2_Value;}else if(this.LEVEL_col === FieldName){return this.SelectedLevel_Value;}else if(this.PARENTID_col === FieldName){return this.SelectedParentId_Value;}else if(this.NODEID_col === FieldName){return this.SelectedNodeId_Value;}else if(this.TEXTID_col === FieldName){return this.SelectedTextId_Value;}else if(this.TEXTDEC_col === FieldName){return this.SelectedTextDesc_Value;}"
      
    },
